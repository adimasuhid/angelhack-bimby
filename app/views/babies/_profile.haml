      - if @photo.present?
        = image_tag @photo.full_url
      - else 
        = image_tag "http://placehold.it/300x240&text=[img]"
    %h5
      %a{:href => "#"}
      =link_to  @baby.name.present? ? @baby.name : "Baby Name", "#"
